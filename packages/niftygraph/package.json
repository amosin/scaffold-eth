{
  "name": "nft-yard",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "codegen": "yarn prepare && graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ ativofinance/nftyardmatic",
    "create-local": "graph create --node http://localhost:8020/ amosin/ativonft",
    "remove-local": "graph remove --node http://localhost:8020/ amosin/ativonft",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 amosin/ativonft",
    "prepare": "mustache ./config/config.json ./src/subgraph.template.yaml > ./subgraph.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.18.0",
    "@graphprotocol/graph-ts": "0.18.0",
    "mustache": "^4.1.0"
  }
}
